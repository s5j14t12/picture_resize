   "source": [
    "import cv2\n",
    "import os\n",
    "import numpy as np\n",
    "hdir = \"./testdata/\"\n",
    "ldir = \"./testdata/\"\n",
    "lis = os.listdir(hdir)\n",
    "for i in range(len(lis)):\n",
    "    image = cv2.imread(hdir + lis[i])\n",
    "    img = cv2.resize(image, (3*image.shape[1], 3*image.shape[0]), interpolation=cv2.INTER_AREA)\n",
    "    cv2.imwrite(ldir + lis[i], img)\n",
    "print(\"done\")"
   ]
